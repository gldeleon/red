@charset "utf-8";

* {
	font-family: Arial, Helvetica, sans;
	font-size: 12px;
	font-weight: bold;
}

html {
	margin: 0px;
	padding: 0px;
	height: 100%;
	overflow: hidden;
}

body {
	margin: 0px;
	padding: 0px;
	height: 100%;
	background-color: #FFF;
}

form {
	margin: 0px;
	padding: 0px;
}

input[type="text"], input[type="password"], textarea, select, option {
	color: #000;
	font-weight: normal;
	border: 1px solid #5E6E65;
	line-height: 18px;
	height: 18px;
	vertical-align: middle;
}

input[type="button"], input[type="submit"] {
	border: none;
	background-color: #811E53;
	color: #FFF;
	width: 75px;
	height: 23px;
	padding-bottom: 2px;
	text-align: center;
	cursor: pointer;
}

input[type="button"].large {
	width: 145px;
}

option {
	border: none;
	height: auto;
	line-height: normal;
}

hr {
	background-color: #8D9C94;
	border: none;
	width: 100%;
	height: 1px;
}

table, td, tr, tbody {
	font-size: inherit;
	font-weight: inherit;
}

